{
  "Version": "2012-10-17",
  "Statement": [
    {
      "Sid": "ReadOnlyAccessToStealthBackup",
      "Effect": "Allow",
      "Action": [
        "s3:GetObject",
        "s3:ListBucket"
      ],
      "Resource": [
        "arn:aws:s3:::stealthbackup",
        "arn:aws:s3:::stealthbackup/*"
      ]
    }
  ]
}
